#include <stdio.h>
int main()
{
    int a[20],i,n,c=0,key;
    printf("Enter the number of elements:");
    scanf("%d",&n);
    printf("Enter the array elements:");
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    printf("Enter the key element:");
    scanf("%d",&key);
    for(i=0;i<n;i++){
        if(a[i]==key){
            c=1;break;
        }
    }
    if(c==1)
        printf("Element found");
    else
        printf("Element not found");
}
